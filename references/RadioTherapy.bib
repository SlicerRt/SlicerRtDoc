@ARTICLE{Zhang2013,
  author = {Zhang, Lanlan and Hub, Martina and Mang, Sarah and Thieke, Christian
	and Nix, Oliver and Karger, Christian P. and Floca, Ralf O.},
  title = {Software for quantitative analysis of radiotherapy: Overview, requirement
