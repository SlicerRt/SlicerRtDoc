  volume = {20},
  pages = {130-137},
  abstract = {Adaptive radiotherapy has been introduced to manage an individual’s
	treatment by, including patient-specific treatment variation identified
	and quantified during the course of
	
	radiotherapy in the treatment planning and delivering optimization.
